# Explicit image definitions to ensure scene names map to files.
# If your Ren'Py version doesn't support WebP, convert to PNG/JPG and update the filenames.

image chairZoomed:
    "chairZoomed.webp"
    zoom 2

image chairUnzoomed:
    "chairUnzoomed.webp"
    zoom 2

image lea default:
    "Lea/lea default.webp"
    zoom .5


image lea defaultZoomed:
    "Lea/lea default.webp"